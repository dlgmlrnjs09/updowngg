    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.3.3",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5"
